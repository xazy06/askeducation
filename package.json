{
  "name": "ask",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node_modules/.bin/nodemon app.js --exec babel-node --",
    "start-mongo-dev": "net start MongoDB",
    "babel-node": "babel-node",
    "clean": "rimraf dist",
    "cleanp": "rimraf -p",
    "prestart": "npm run -s build",
