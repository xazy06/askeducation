  },
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "mongoose": "^5.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-sass-middleware": "0.11.0",
    "sharp": "^0.23.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "cors": "^2.8.5",
    "nodemon": "^1.19.1"
  }
}
